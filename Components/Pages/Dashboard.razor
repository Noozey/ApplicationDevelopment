@page "/Dashboard"

<div id="analyticsView" class="space-y-6">
 <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-4">
        <div class="bg-white rounded-lg shadow p-6">
            <div class="flex items-center justify-between mb-2">
                <h3 class="text-sm font-medium text-secondary-foreground">Current Streak</h3>
                <svg class="text-info" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><polyline points="23 6 13.5 15.5 8.5 10.5 1 18"></polyline><polyline points="17 6 23 6 23 12"></polyline></svg>
            </div>
            <p class="text-3xl font-bold text-foreground">7 days</p>
        </div>
        <div class="bg-white rounded-lg shadow p-6">
            <div class="flex items-center justify-between mb-2">
                <h3 class="text-sm font-medium text-secondary-foreground">Longest Streak</h3>
                <svg class="text-success" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><rect x="3" y="4" width="18" height="18" rx="2" ry="2"></rect><line x1="16" y1="2" x2="16" y2="6"></line><line x1="8" y1="2" x2="8" y2="6"></line><line x1="3" y1="10" x2="21" y2="10"></line></svg>
            </div>
            <p class="text-3xl font-bold text-foreground">23 days</p>
        </div>
        <div class="bg-white rounded-lg shadow p-6">
            <div class="flex items-center justify-between mb-2">
                <h3 class="text-sm font-medium text-secondary-foreground">Total Entries</h3>
                <svg class="text-accent" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M11 4H4a2 2 0 0 0-2 2v14a2 2 0 0 0 2 2h14a2 2 0 0 0 2-2v-7"></path><path d="M18.5 2.5a2.121 2.121 0 0 1 3 3L12 15l-4 1 1-4 9.5-9.5z"></path></svg>
            </div>
            <p class="text-3xl font-bold text-foreground">142</p>
        </div>
        <div class="bg-white rounded-lg shadow p-6">
            <div class="flex items-center justify-between mb-2">
                <h3 class="text-sm font-medium text-secondary-foreground">Missed Days</h3>
                <svg class="text-destructive" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><line x1="18" y1="6" x2="6" y2="18"></line><line x1="6" y1="6" x2="18" y2="18"></line></svg>
            </div>
            <p class="text-3xl font-bold text-foreground">8</p>
        </div>
    </div>

    <div class='grid gap-4 mx-4'>
        <div></div>
        <div class="grid grid-cols-1 lg:grid-cols-2 gap-6">
            <div class="bg-white rounded-lg shadow p-6">
                <div class="flex justify-between items-center mb-4">
                    <h3 class="text-lg font-semibold text-foreground">Mood Distribution</h3>
                    <svg class="text-tertiary-foreground" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M21.21 15.89A10 10 0 1 1 8 2.83"></path><path d="M22 12A10 10 0 0 0 12 2v10z"></path></svg>
                </div>
                <div class="space-y-3">
                    <div>
                        <div class="flex justify-between text-sm mb-1">
                            <span class="text-secondary-foreground">Positive</span>
                            <span class="font-medium text-success-foreground">65%</span>
                        </div>
                        <div class="w-full bg-muted rounded-full h-3">
                            <div class="border border-success h-3 rounded-full" style="width: 65%;"></div>
                        </div>
                    </div>
                    <div>
                        <div class="flex justify-between text-sm mb-1">
                            <span class="text-secondary-foreground">Neutral</span>
                            <span class="font-medium text-info-foreground">25%</span>
                        </div>
                        <div class="w-full bg-muted rounded-full h-3">
                            <div class="border border-info h-3 rounded-full" style="width: 25%;"></div>
                        </div>
                    </div>
                    <div>
                        <div class="flex justify-between text-sm mb-1">
                            <span class="text-secondary-foreground">Negative</span>
                            <span class="font-medium text-destructive-foreground">10%</span>
                        </div>
                        <div class="w-full bg-muted rounded-full h-3">
                            <div class="border-destructive border h-3 rounded-full" style="width: 10%;"></div>
                        </div>
                    </div>
                </div>
            </div>


        </div>

        <div class="grid grid-cols-1 lg:grid-cols-2 gap-6">
            <div class="bg-white rounded-lg shadow p-6">
                <div class="flex justify-between items-center mb-4">
                    <h3 class="text-lg font-semibold text-foreground">Most Used Tags</h3>
                    <svg class="text-tertiary-foreground" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M20.59 13.41l-7.17 7.17a2 2 0 0 1-2.83 0L2 12V2h10l8.59 8.59a2 2 0 0 1 0 2.82z"></path><line x1="7" y1="7" x2="7.01" y2="7"></line></svg>
                </div>
                <div class="flex flex-wrap gap-3">
                    <span class="px-4 py-2 bg-muted text-accent-foreground rounded-lg border border-accent" style="font-size: 16px;">Work (45)</span>
                    <span class="px-4 py-2 bg-muted text-accent-foreground rounded-lg border border-accent" style="font-size: 15px;">Health (40)</span>
                    <span class="px-4 py-2 bg-muted text-accent-foreground rounded-lg border border-accent" style="font-size: 14px;">Personal Growth (35)</span>
                    <span class="px-4 py-2 bg-muted text-accent-foreground rounded-lg border border-accent" style="font-size: 13px;">Exercise (30)</span>
                    <span class="px-4 py-2 bg-muted text-accent-foreground rounded-lg border border-accent" style="font-size: 12px;">Family (25)</span>
                    <span class="px-4 py-2 bg-muted text-accent-foreground rounded-lg border border-accent" style="font-size: 11px;">Reading (20)</span>
                </div>
            </div>
        </div>
        <div></div>
    </div>

    
</div>